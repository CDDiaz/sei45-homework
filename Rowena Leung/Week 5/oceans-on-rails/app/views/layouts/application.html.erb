<!DOCTYPE html>
<html>
  <head>
    <title>Oceans On Rails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">

    <nav>
      <%= link_to 'Home', root_path %>
    <%= link_to"See all", oceans_path%>
    <%= link_to "Add new", new_ocean_path %>
    </nav>
    <%= yield %>
    </div>
  </body>
</html>
