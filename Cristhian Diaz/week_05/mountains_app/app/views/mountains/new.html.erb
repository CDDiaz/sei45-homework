<h2>Add Mountain</h2>

<form action="<%= mountains_path %>" method="post">

<label>
  Name:
  <input name="name" required>
</label>

<label>
  Range:
  <input name="range">
</label>

<label>
  Location:
  <input name="location">
</label>

<label>
  Prominence:
  <input type="number" name="prominence">
</label>

<label>
  Image:
  <input type="url" name="image">
</label>

<button>Create Mountain</button>

</form>
